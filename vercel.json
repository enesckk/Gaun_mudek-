{
  "version": 2,
  "builds": [
    {
      "src": "api/index.mjs",
      "use": "@vercel/node",
    },
    {
      "src": "frontend/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.mjs"
    },
    {
      "src": "/(.*)",
      "dest": "frontend/$1"
    }
  ],
      "installCommand": "cd api && npm install && cd ../backend && npm install && cd ../frontend && npm install"
}

